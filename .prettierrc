{
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react(-native)?$",
        "^@?\\w",
        "^@/components/(.*)$",
        "^@/data/(.*)$",
        "^@/utils/(.*)$",
        "^@/types/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
