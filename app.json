{
    "expo": {
        "name": "loot-reaver",
        "slug": "loot-reaver",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "lootreaver",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "jsEngine": "hermes",
            "supportsTablet": true
        },
        "android": {
            "jsEngine": "hermes",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-web-browser",
            "expo-secure-store",
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "e81c6099-c11d-4caa-8af5-e6e7f81b7e29"
            }
        },
        "owner": "alexander90",
        "originalFullName": "@alexander90/loot-reaver",
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/e81c6099-c11d-4caa-8af5-e6e7f81b7e29"
        }
    }
}
